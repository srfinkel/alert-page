<h1>A listing of all the posts</h1>

<table>
<tr>
<th>Date Created</th>
<th>Title</th>
<th>Description</th>
</tr>
<% @posts.each do | post| %>
<tr>
<td><%= post.created_at %></td>
<td><%= post.title %></td>
<td><%= post.description %></td>
<td><%= link_to 'Edit', edit_post_path(post) %></td>
<td><%= link_to 'Show', post_path(post) %></td>
</tr>
<% end %>
</table>